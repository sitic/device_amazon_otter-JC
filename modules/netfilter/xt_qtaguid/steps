export YOUR_PATH=~/android/d.amazon-otter
export MYDROID=$YOUR_PATH/mydroid
export PATH=$PATH:$YOUR_PATH/build_tools/arm-2010q1/bin
export JAVA_HOME=/usr/lib/jvm/java-6-sun
